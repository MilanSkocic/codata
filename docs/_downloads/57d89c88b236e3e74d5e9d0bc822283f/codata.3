/* EXAMPLE IN C */
#include <stdio.h>
#include \(dqcodata.h\(dq

int main(void){
    
    printf(\(dq########## EXAMPLE IN C ##########\en\(dq);

    printf(\(dq%s\en\(dq,\(dq# VERSION\(dq);
    printf(\(dqversion = %s\en\(dq, codata_get_version());
    
    printf(\(dq%s\en\(dq,\(dq# CONSTANTS\(dq);
    printf(\(dqc = %f\en\(dq, SPEED_OF_LIGHT_IN_VACUUM.value);
    
    printf(\(dq%s\en\(dq,\(dq# UNCERTAINTY\(dq);
    printf(\(dqu(c) = %f\en\(dq, SPEED_OF_LIGHT_IN_VACUUM.uncertainty);
    
    printf(\(dq%s\en\(dq,\(dq# OLDER VALUES\(dq);
    printf(\(dqMu_2022(latest) = %23.16f\en\(dq, MOLAR_MASS_CONSTANT.value);
    printf(\(dqMu_2018 = %23.16f\en\(dq, MOLAR_MASS_CONSTANT_2018.value);
    printf(\(dqMu_2014 = %23.16f\en\(dq, MOLAR_MASS_CONSTANT_2014.value);
    printf(\(dqMu_2010 = %23.16f\en\(dq, MOLAR_MASS_CONSTANT_2010.value);

    return 0;
}

