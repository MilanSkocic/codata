.SH EXAMPLES
.SS Fortran
.INDENT 0.0
.INDENT 3.5
.sp
.EX
! EXAMPLE IN FORTRAN
program example_in_f
    use iso_fortran_env
    use codata
    implicit none
    
    print \(aq(A)\(aq, \(aq########## EXAMPLE IN FORTRAN ##########\(aq
    
    print \(aq(A)\(aq, \(aq# VERSION\(aq
    print *, \(dqversion = \(dq, get_version()
    
    print \(aq(A)\(aq, \(aq# CONSTANTS\(aq
    print *, \(dqc = \(dq,  SPEED_OF_LIGHT_IN_VACUUM%value
    
    print \(aq(A)\(aq, \(aq# UNCERTAINTY\(aq
    print *, \(dqu(c) = \(dq, SPEED_OF_LIGHT_IN_VACUUM%uncertainty

    print \(aq(A)\(aq, \(aq# OLDER VALUES\(aq
    print \(aq(A, F23.16)\(aq, \(dqMu_2022(latest) = \(dq, MOLAR_MASS_CONSTANT%value
    print \(aq(A, F23.16)\(aq, \(dqMu_2018 = \(dq, MOLAR_MASS_CONSTANT_2018%value
    print \(aq(A, F23.16)\(aq, \(dqMu_2014 = \(dq,  MOLAR_MASS_CONSTANT_2014%value
    print \(aq(A, F23.16)\(aq, \(dqMu_2010 = \(dq,  MOLAR_MASS_CONSTANT_2010%value


end program

